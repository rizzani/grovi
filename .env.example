# Appwrite Configuration
# Copy this file to .env and fill in your values

# Appwrite Endpoint (e.g., https://cloud.appwrite.io/v1)
EXPO_PUBLIC_APPWRITE_ENDPOINT=

# Appwrite Project ID (found in your Appwrite project settings)
EXPO_PUBLIC_APPWRITE_PROJECT_ID=

# Appwrite Admin API Key (required for database setup script)
# Get this from: Appwrite Console > Settings > API Keys > Create API Key
# Make sure to select 'Admin' scopes
APPWRITE_API_KEY=

# Database ID (optional, defaults to 'grovi-db' if not provided)
APPWRITE_DATABASE_ID=grovi-db
